
public class TestApp4 {

	public static void main(String[] args) {
		String s1 = new String("sachin");
		s1.concat("tendulkar");
		s1 += "IND";
		String s2 = s1.concat("MI");
		System.out.println(s1);
		System.out.println(s2);
		
		for (int i=0;i<=100;i++) {
			if(i%3==0)
			System.out.println(i);
		}
	}

}
