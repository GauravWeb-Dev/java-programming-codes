public class Launch {
	
    public void greet(String name) {
    	System.out.println("Entering greet method of launch");
    	System.out.println("Good Morning " + name);
    	
    	Demo d = new Demo();
    	d.add(22, 44);
    	
    	System.out.println("Exiting greet method of launch");

    }
}
