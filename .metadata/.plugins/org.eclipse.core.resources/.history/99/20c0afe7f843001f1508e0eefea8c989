
class Student{
	private int id;
	private String name;
	
	public void setId(int id) {
		this.id=id;
	}
	
	public void setName(String name) {
		this.name=name;
	}
	
	public int getId() {
		return id;
	}
	
	public String getName() {
		return name;
	}
	
	
}

public class Test2 {

	public static void main(String[] args) {
        
		Student s = new Student();
		s.setId(1);
		s.setName("Sachin");
		
		System.out.println(s);
		
	}

}
