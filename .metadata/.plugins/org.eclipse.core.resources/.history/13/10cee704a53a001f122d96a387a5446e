
public class Test6 {

	public static void main(String[] args) {
         float x = 10f;
         float y = 20f;
         
         float sum =(x + y) ;
         System.out.println(sum);
         
         float sub = x - y ;
         System.out.println(sub);
         
         float mul = x * y ;
         System.out.println(mul);
         
         float div = x / y ;
         System.out.println(div);
	}

}
