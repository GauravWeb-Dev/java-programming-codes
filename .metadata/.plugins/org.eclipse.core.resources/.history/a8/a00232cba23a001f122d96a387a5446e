
public class Test5 {
    public static void main(String[] args) {
		
    	
    	byte b = 100;
    	
    	int x = b;
    	
    	System.out.println(x);
    	
    	
    	int x1 = 125;
    	
    	float f = x1;
    	System.out.println(f);
    	
    	float f1= 12.56f;
    	
    	double d = f1;
    	
    	short s1 = 2356;
    	
    	long l1 = s1;
    	
    	
    	float f3 = 12.5f;
    	
    	int x3 =(int)f3;
    	
    	System.out.println(x3);
    	
    	int x4 = 5000;
    	
    	short s2 =(short) x4;
    	System.out.println(s2);
    	
    	
    	int x2 = 300;
    	
    	byte b1 =(byte) x2;
    	
    	System.out.println(b1);
    	
    	
    	byte z = 98;
    	
    	char c =(char) z;
    	
    	System.out.println(c);
    	
	}
}
