
public class LetterV {
	public static void main(String[] args){
	
     // create scanner class to take user input
    
		int n=10;
     int x = 1;
     // store n*2-1 value in y
     int y = n * 2 - 1;
       
     // loop to iterate through ns
     for (int i = 0; i < n; i++)
     {
       // iterate inner loop from 1 till n*2
       for (int j = 0; j < n * 2; j++)
       {
         // if c= x or y print the symbol, else space 
         if (j == x || j == y)
         {
           System.out.print("*");
         }
         else
         {
           System.out.print(" ");
         }
       }
       // increment x
       x++;
       // decrement y
       y--;
       System.out.println("");
     }        
}
}
