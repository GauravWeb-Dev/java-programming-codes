package com.solution.service;

class Test{
	static int count;
	{
		count++;
        }

	Test(){

	}
	
	Test(int a){
		
	}

	Test(int a,int b){
		
	}

	Test(int a,int b, int c){
		
	}

	public void display(){
		System.out.println("No of objects created is ::"+count);
	}
}
public class TestApp{
	public static void main(String... args){
		new TestApp();
		new TestApp(10);
		new TestApp(10,20);
		TestApp t= new TestApp(10,20,30);
		t.display();
	}
}