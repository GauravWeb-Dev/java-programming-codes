
public class Launch2 {

	public static void main(String[] args) {
        //Implicit TypeCasting
		byte b = 100;
         
         int x = b;
         
         System.out.println(x);
         
         long l = 1234567890l;
         
         double d = l;
          System.out.println(d);
         
          char c = ' ';
          
          int x1 = c;
          System.out.println(x1);
          
          //Explicit TypeCasting
          
          int x2 = 150;
          
          byte b2 =(byte) x2;
          
          System.out.println(b2);
          
          int i = 400;
          
          byte a =(byte) i;
          
          System.out.println(a);
          
          
          
          
          
          
          
          
       
		
	}

}
