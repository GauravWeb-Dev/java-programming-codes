
public class Test2 {

	public static void main(String[] args) {
         /*
          * Initialization;
          * 
          * while(condition){
          *  Body
          * 
          * Increment;
          * } 
          * 
          */
		
		
		int i = 0;
		
		while(i<=10) {
			System.out.println(i);
			
			i++;
		}
		
		while(true) {
			
			System.out.println("Hello World !!");
		}
		
	}

}
