class Calculator{
	
	public void add(int a , int b) {
		int sum  = a+b;
		System.out.println(sum);
	}
	
	public void sub (int a ,int b) {
		int sub = a-b;
		System.out.println(sub);
	}
	
	public void mul(int a , int b) {
		int mul = a*b;
		System.out.println(mul);
	}
	
	public void div(int a ,int b) {
		int div=a/b;
		System.out.println(div);
	}
	
	
}


