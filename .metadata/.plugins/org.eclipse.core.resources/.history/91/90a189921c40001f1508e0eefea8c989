
public class Test2 {

	public static void main(String[] args) {
       
		String s = "Sachin";
		
		System.out.println(s);
		
		s = s.concat("Tendulkar");
		
	
		System.out.println(s);
		
		
		String s1 = new String("Sachin");
		
		
		StringBuilder s2 = new StringBuilder("Virat");
		
		s2.append("Kohli");
		
		System.out.println(s2);
		
	}

}
