
public class Launch2 {

	public static void main(String[] args) {
        //Implicit TypeCasting
		byte b = 100;
         
         int x = b;
         
         System.out.println(x);
         
         long l = 1234567890l;
         
         double d = l;
          System.out.println(d);
         
          char c = ' ';
          
          int x1 = c;
          System.out.println(x1);
          
          
          
          
       
		
	}

}
