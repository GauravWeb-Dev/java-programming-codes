
public class Test3 {

	public static void main(String[] args) {
       
		byte b = 100;
		
		int x = b;
		
		float f = 12.5f;
		
		double d = f;
		
		short s = 10000;
		
		long l =s;
		
		
		int x1 = 1000;
		
		byte b1 =(byte) x1;
		System.out.println(b1);
		
		float f1 = 12.5f;
		
		int x2 =(int) f1;
		
		int x3 = 160;
		
		byte b3 = (byte)x3;
		
		System.out.println(b3);
		
		
		System.out.println(x2);
		
		byte b2 = 100;
		
		char c =(char) b2;
		
		System.out.println(c);
		
		
		int z = 10;
		
		System.out.println(z++);
		
		System.out.println(z);
		
		
	}

}
