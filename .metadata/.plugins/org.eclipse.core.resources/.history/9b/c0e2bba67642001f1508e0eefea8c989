
public class Demo {

	public static void main(String[] args) {
       
		
		String name[] = {"salman","akshay","virat","katrina","shradhha","Priyanka" };
		
		for(String s : name) {
			System.out.println(s);
		}
		
		for(int i =0;i<name.length;i++) {
			System.out.println(name[i]);
		}
		
		
		
		
	

		
	}

}
